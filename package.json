  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run build:prod",
    "build:prod": "npm run clean && npm run build:css:prod && cross-env NODE_ENV=production webpack",
    "build:dev": "npm run clean && npm-run-all -p watch:css watch:js",
    "watch:css": "npx tailwindcss -i ./tools/css/app.css -o ./tools/css/style.css --watch",
    "watch:js": "cross-env NODE_ENV=development webpack --watch",
    "build:css:prod": "tailwindcss -i ./tools/css/app.css -o ./dist/tools/css/style.css --minify"
